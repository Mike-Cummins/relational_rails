<h1><%= @dealership.name%>'s Inventory</h1>
<a href="/dealerships/<%= @dealership.id %>/vehicles/new">Create Vehicle</a>
  <% @vehicles.each do |vehicle| %>
    <h3>Make: <%= vehicle.make %>
      Model: <%= vehicle.model %></h3>
    <p>Year: <%= vehicle.year %></p>
    <p>Sale Pending: <%= vehicle.sale_pending %></p>
  <% end %>
  <a href="/dealerships/<%= @dealership.id %>/vehicles?sort_by_alpha=true">Sort</a>